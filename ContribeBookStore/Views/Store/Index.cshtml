@model IEnumerable<ContribeBookStore.Models.Genre>
@{
    ViewBag.Title = "Store";
}
<h3>Browse Genres</h3>

<p>
    Select from @Model.Count() genres:
</p>
<ul class="selling">
    @foreach (var genre in Model)
    {
        <li style="padding-right:20em" >@Html.ActionLink(genre.Name, "Browse", new { genre = genre.Name }) <br /></li>
    }
</ul>